<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ocft.gateway.mapper.MessageConverterMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ocft.gateway.entity.MessageConverter">
        <result column="id" property="id" />
        <result column="name" property="name" />
        <result column="url" property="url" />
        <result column="backon_url" property="backonUrl" />
        <result column="request_config" property="requestConfig" />
        <result column="response_config" property="responseConfig" />
        <result column="request_struct" property="requestStruct" />
        <result column="response_struct" property="responseStruct" />
        <result column="status" property="status" />
        <result column="is_deleted" property="isDeleted" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="create_by" property="createBy" />
        <result column="update_by" property="updateBy" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,name,url,backon_url,request_config, response_config,request_struct,response_struct,status,is_deleted,create_time,update_time,create_by,update_by
    </sql>

</mapper>
